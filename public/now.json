{
  "version": 2,
  "alias": "ashleys-artiste-client",
  "name": "artiste-client",  
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/manifest.json",
        "dest": "/manifest.json"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}