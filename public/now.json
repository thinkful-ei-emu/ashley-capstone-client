{
  "version": 2,
  "alias": "ashleys-artiste-client",
  "name": "artiste-client",  
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },  
      {
        "src": "^/favicon.ico$",
        "dest": "/favicon.ico"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}